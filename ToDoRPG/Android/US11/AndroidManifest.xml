<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.CS429.newtodorpg"
    android:installLocation="preferExternal"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_character"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.CS429.newtodorpg.CharacterActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.CS429.newtodorpg.HabitActivity" />
        <activity android:name="com.CS429.newtodorpg.ToDoActivity" />
        <activity android:name="com.CS429.newtodorpg.InventoryActivity" />
        <activity android:name="com.CS429.newtodorpg.RewardsActivity" />
        <activity
            android:name="com.CS429.newtodorpg.DailyActivity"
            android:label="@string/title_activity_daily" >
        </activity>
        <activity
            android:name="com.CS429.newtodorpg.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>